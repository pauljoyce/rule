<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.clinical.dao.master.ZjInterventionTherapyMapper">
<select id="findZjInterventionTherapyByUniqueId"  resultType="com.clinical.model.master.INTERVENTION_THERAPY">
       SELECT * FROM INTERVENTION_THERAPY WHERE unique_id_lv2 = #{unique_id_lv2}
</select>

<select id="findZjInterventionTherapyByIncr"  resultType="java.lang.String">
       select distinct(unique_id_lv2) FROM INTERVENTION_THERAPY where date(created_at) = date_sub(curdate(),interval 1 day) 
</select>


</mapper>