<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.clinical.dao.master.ZjEmergencyRecordMapper">
<select id="findZjEmergencyRecordByUniqueId"  resultType="com.clinical.model.master.EMERGENCY_RECORD">
       SELECT * FROM EMERGENCY_RECORD WHERE unique_id_lv2 = #{unique_id_lv2}
</select>

<select id="findZjEmergencyRecordByIncr"  resultType="java.lang.String">
       select distinct(unique_id_lv2) FROM EMERGENCY_RECORD where date(created_at) = date_sub(curdate(),interval 1 day) 
</select>


</mapper>