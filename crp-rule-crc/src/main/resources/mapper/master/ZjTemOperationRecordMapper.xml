<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.clinical.dao.master.ZjTemOperationRecordMapper">
<select id="findZjTemOperationRecordByUniqueId"  resultType="com.clinical.model.master.TEM_OPERATION_RECORD">
       SELECT * FROM TEM_OPERATION_RECORD WHERE unique_id_lv2 = #{unique_id_lv2}
</select>
       <select id="findZjTemOperationRecordByALL"  resultType="com.clinical.model.master.TEM_OPERATION_RECORD">
       SELECT * FROM TEM_OPERATION_RECORD
</select>

       <select id="findZjTemOperationRecordByUniqueIdOrderByItemEndDate"  resultType="com.clinical.model.master.TEM_OPERATION_RECORD">
       SELECT * FROM TEM_OPERATION_RECORD WHERE unique_id_lv2 = #{unique_id_lv2} order by ITEM_END_DATE
</select>

       <select id="findZjTemOperationRecordByUniqueId2"  resultType="com.clinical.model.master.TEM_OPERATION_RECORD">
       SELECT * FROM TEM_OPERATION_RECORD WHERE unique_id = #{unique_id}
</select>
</mapper>