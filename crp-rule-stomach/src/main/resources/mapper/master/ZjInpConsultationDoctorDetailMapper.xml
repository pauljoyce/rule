<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.clinical.dao.master.ZjInpConsultationDoctorDetailMapper">
<select id="findZjInpConsultationDoctorDetailByUniqueId"  resultType="com.clinical.model.master.INP_CONSULTATION_DOCTOR_DETAIL">
       SELECT * FROM INP_CONSULTATION_DOCTOR_DETAIL WHERE unique_id_lv2 = #{unique_id_lv2}
</select>

<select id="findZjInpConsultationDoctorDetailByIncr"  resultType="java.lang.String">
       select distinct(unique_id_lv2) FROM INP_CONSULTATION_DOCTOR_DETAIL where date(record_time) = date_sub(curdate(),interval 1 day) 
</select>


</mapper>